{
  "name": "auctioneer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tailwindcss -i ./src/css/tailwind.css -o ./dist/output.css",
    "watch": "npx tailwindcss -i ./src/css/tailwind.css -o ./dist/output.css --watch & live-server",
    "test": "npm run test-unit && npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-report": "cypress run --reporter mochawesome",
    "test-e2e-cli": "cypress run",
    "prepare": "husky install",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "dev": "vite",
    "vite-build": "vite build",
    "vite-preview": "vite preview",
    "docs": "jsdoc -c ./conf.json -r"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pretzL/Auctioneer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pretzL/Auctioneer/issues"
  },
  "homepage": "https://github.com/pretzL/Auctioneer#readme",
  "devDependencies": {
    "cypress": "^10.7.0",
    "eslint": "^8.27.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.2",
    "jest": "^29.2.0",
    "jsdoc": "^4.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "bootstrap-icons": "^1.10.2",
    "daisyui": "^2.41.0",
    "dayjs": "^1.11.6",
    "flowbite": "^1.5.4",
    "tailwindcss": "^3.2.4",
    "vite": "^3.2.4"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "eslint --cache --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
